#include <iostream>
using namespace std;

int main() {
    int edades[] = {15, 12, 23, 30};

    try {
        cout << "La edad de la posición 4 es: " << edades[4] << endl;
    } catch (const out_of_range& e) {
        cout << "Intentaste acceder a un índice que no existe" << endl;
    }

    return 0;
}
